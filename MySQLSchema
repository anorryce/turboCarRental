CREATE TABLE Vehicle (
VIN int(10) primary key,
Price decimal (3,2) not null,
LicensePlateNum varchar(7) not null,
Longitude varchar(10),
Latitude varchar(10),
Mileage bigint,
vColor varchar(10),
vYear int(4) not null,
vMake varchar(20) not null,
vModel varchar(20) not null,
vType varchar(10),
vCondition varchar(20),
storeNum int(3),
RentedUntil date
);

CREATE TABLE Employee (
SSN int(9) primary key,
superSSN int(9) not null,
DOB date,
address varchar(50),
city varchar(50),
state varchar(2),
zip varchar(5),
phoneNum varchar(10) not null,
email varchar(50),
storeNum int(3),
salary decimal(5,2) not null,
startDate date,
employeeID varchar(5) not null,
jobType varchar(20)
);

CREATE TABLE Customer (
Email varchar(50) primary key,
Username varchar(50) primary key,
FName varchar(30) not null,
LName varchar(30) not null,
Mint varchar(1),
address varchar(50),
city varchar(50),
state varchar(2),
zip varchar(5),
password varchar(20) not null
phoneNum varchar(10)
);

CREATE TABLE Store (
storeNum int(3) primary key,
phoneNum varchar(10) not null,
address varchar(50),
city varchar(50),
state varchar(2),
zip varchar(5)
);
