CREATE TABLE Vehicle (
VIN varchar(20),
Price decimal (3,2) not null,
LicensePlateNum varchar(7) not null,
Mileage number(6),
vColor varchar(10),
vYear number(4) not null,
vMake varchar(20) not null,
vModel varchar(20) not null,
vType varchar(10),
vCondition varchar(20),
storeNum number(4),
RentedUntil date,
primary key(VIN),
foreign key(storeNum) references Store(storeNum)
);

CREATE TABLE Employee (
FName varchar(50),
Mint char(1),
LName varchar(50),
SSN number(9) not null,
superSSN number(9),
DOB date,
address varchar(50),
city varchar(50),
state char(2),
zip number(5),
phoneNum varchar(20) not null,
email varchar(50),
storeNum number(4),
salary decimal(5,2) not null,
startDate date,
employeeID number(2) not null,
jobType varchar(20),
primary key (SSN),
foreign key (storeNum) references Store (storeNum),
foreign key (superSSN) references Employee (SSN)
);

CREATE TABLE Customer (
email varchar(50),
userName varchar(50),
FName varchar(30) not null,
LName varchar(30) not null,
Mint char(1),
address varchar(50),
city varchar(50),
state char(2),
zip number(5),
password varchar(20) not null,
phoneNum varchar(20),
primary key (email, userName)
);

CREATE TABLE Store (
storeNum number(4),
phoneNum varchar(20) not null,
address varchar(50),
city varchar(50),
state char(2),
zip number(5),
primary key (storeNum)
);

CREATE TABLE Transaction (
userName varchar(50),
amountCharged decimal(3,2),
payMethod varchar(20),
checkOutDate date,
checkInDate date,
cardNumber number(20),
primary key (userName),
foreign key (userName) references Customer (userNamer)
);
